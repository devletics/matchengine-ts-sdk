{
  "name": "@devletics/matchengine-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for MatchEngine booking API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "matchengine",
    "booking",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "Devletics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devletics/matchengine-ts-sdk"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "luxon": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "luxon": {
      "optional": true
    }
  }
}
